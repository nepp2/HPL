struct point {
  x : i64
  y : i64
}
fun foo(a : point, b : point) {
  point(x: a.x + b.x, y: a.y + b.y)
}
let a = point(x: 10, y: 1)
let b = point(x: 2, y: 20)
let c = foo(a, b)
